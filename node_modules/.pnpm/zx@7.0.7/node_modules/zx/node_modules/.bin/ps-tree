#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/snowy/mycode/stars/create-dc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/snowy/mycode/stars/create-dc/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ps-tree@1.2.0/node_modules/ps-tree/bin/ps-tree.js" "$@"
else
  exec node  "$basedir/../../../../../ps-tree@1.2.0/node_modules/ps-tree/bin/ps-tree.js" "$@"
fi
